package co.micol.miniproject.member.service;

import java.util.List;

public interface MemberService {  //CRUD
	List<MemberVO> memberSelectList();  //멤버전체 조회
	MemberVO memberSelect(MemberVO vo); //한명 멤버 조회
	int memberInsert(MemberVO vo);  //멤버 등록
	int memberDelete(MemberVO vo);  //멤버 삭제
	int memberUpdate(MemberVO vo);  //멤버 변경
}
